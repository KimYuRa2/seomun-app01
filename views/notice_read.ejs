<main class="notice_read_main">
        <div class="notice_read">
            <h2>공지사항</h2>
        </div>

        <section class="notice_read_section">
            <form method="GET">
                
                <div class="notice_read_title">
                    <h4><%=row.cate%></h4>
                    <h3><%=row.title%></h3>
                    <ul class="noti_date">
                        <li>등록일</li>
                        <li><%=row.create_time%></li>
                    </ul>
                    <ul class="noti_date">
                        <li>수정일</li>
                        <li><%=row.update_time%></li>
                    </ul>
                </div>

                <div class="notice_read_content">
                    <p class="content">
                        <%=row.content%>
                    </p>
                    <p class="file_download">
                        <a href="">품평회 심사 대상자 2022_04..pdf</a>
                    </p>
                    <p class="file_download">
                        <a href="">품평회 심사 대상자 2022_04..pdf</a>
                    </p>
                </div>

                <section class="noti_list_wrap">
                    <div class="noti_list_btn1">
                        <div onclick="javascript:location.href='/updateNotice?id=<%=row.id%>'">수정</div>
                        <div onclick="javascript:location.href='/deleteNotice?id=<%=row.id%>'">삭제</div>
                    </div>
                    <div class="noti_list_btn2">
                        <div>
                            <a href="notice">목록보기</a>
                        </div></div>
                </section>
                
            </form>
        </section>
</main>